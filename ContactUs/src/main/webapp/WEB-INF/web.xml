<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ContactUs</display-name>
   <servlet>
   <servlet-name>Home</servlet-name>
   <jsp-file>/Index.jsp</jsp-file>
   </servlet>
   <servlet-mapping>
     <servlet-name>Home</servlet-name><url-pattern>/</url-pattern>
   </servlet-mapping>
<servlet>
   <servlet-name>Contactus</servlet-name>
   <jsp-file>/Contactus.jsp</jsp-file>
   </servlet>
   <servlet-mapping>
     <servlet-name>Contactus</servlet-name>
     <url-pattern>/contactus</url-pattern>
   </servlet-mapping>
<servlet>
<servlet-name>admin/login</servlet-name>
   <jsp-file>/AdminUs.jsp</jsp-file>
   </servlet>
   <servlet-mapping>
     <servlet-name>admin/login</servlet-name>
     <url-pattern>/admin/login</url-pattern>
   </servlet-mapping>
 


</web-app>